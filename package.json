{
  "author": "Mikhail Shuvalov",
  "version": "2.2.0",
  "description": "Starter project template boilerplate based on Webpack with SASS/PostCSS and babel ES6/7 support.",
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@popperjs/core": "^2.10.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.1.3",
    "imagemin-svgo": "^10.0.0",
    "jquery-nice-select": "^1.1.0",
    "magnific-popup": "^1.1.0",
    "rucksack-css": "^1.0.2",
    "tiny-slider": "^2.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.16.1",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@typescript-eslint/parser": "^5.3.1",
    "ajv": "^8.7.1",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-preset-typescript-vue": "^1.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.2",
    "ejs": "^3.1.6",
    "ejs-loader": "^0.5.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.6",
    "mini-css-extract-plugin": "^2.4.4",
    "mocha": "^9.1.3",
    "mocha-loader": "^5.1.5",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "./local_modules/postcss-preset-env-next-release",
    "sass": "^1.43.4",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.3.0",
    "sass-rem": "^3.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.0.1",
    "stylelint-webpack-plugin": "^3.1.0",
    "svgo-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "typescript": "^4.4.4",
    "vue-loader": "^15.9.8",
    "webpack": "^5.63.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "setup"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:dev": "webpack --config configuration/webpack.dev.config.js --mode=development",
    "watch": "webpack --config configuration/webpack.dev.config.js --mode=development --watch",
    "bundle": "npm install && npm run watch",
    "serve": "webpack serve --config configuration/webpack.dev.config.js --mode=development",
    "build": "webpack --config configuration/webpack.prod.config.js --mode=production",
    "build:wp": "webpack --config configuration/webpack.wp.config.js --mode=production",
    "build:dev_wp": "webpack --config configuration/webpack.wp.config.js --mode=development",
    "lint:sass": "sass-lint -v -q",
    "lint:js": "eslint --ext .js src/assets/js/",
    "stats": "webpack --config configuration/webpack.prod.config.js --mode=production --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
  "target": "web"
}
